"unlitgeneric"
{
	"$basetexture" "yorch/vidagif/LowHealthPlayerNPC"
	"$model" "1"
	"$translucent" 1
	"$additive" 1
	"$color" "[1 1 1]" // R, G, B son valores de color en formato decimal (0-255)

		"proxies"
	{
		"AnimatedOffsetTexture"
			{
			"Animatedtexturevar" "$Basetexture"
			"Animatedtextureframenumvar" "$frame"
			"Animatedtextureframerate" "30"
			}
	}

}
